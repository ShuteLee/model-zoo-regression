//===-*- Groovy -*-===

pipeline {
    parameters {
        string(name: 'JSON_PARAM', defaultValue: '', description: 'json')
    }
    agent {
        label 'linux-sc5-sc7'
    }
    stages {
        stage('Information') {
            steps {
                println("start profile parsing...")
            }
        }
        stage('Process') {
            steps {
                dir("$WORKSPACE") {
                    script {
                        println("params: ${params.JSON_PARAM}")
                    }
                }
            }
        }
    }
}